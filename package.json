{
  "name": "git4school",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-prod": "ng build git4school --configuration=production --statsJson",
    "doc": "npx compodoc -p tsconfig.json",
    "serve-doc": "npx compodoc -p tsconfig.json --theme Readthedocs -n \"Git4School documentation\" --includes ./src/assets/additional-docs/ -d docs -a ./src/assets/additional-docs/ -s -w",
    "build:doc": "npm run-script build-prod && npm run-script doc",
    "update": "ng update --all --force"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/localize": "^9.0.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@swimlane/ngx-datatable": "^17.0.0",
    "ajv": "^6.12.0",
    "bootstrap": "4.4.1",
    "chart.js": "^2.9.3",
    "chartjs-plugin-annotation": "^0.5.7",
    "chartjs-plugin-colorschemes": "^0.4.0",
    "chartjs-plugin-datalabels": "^0.7.0",
    "chartjs-plugin-zoom": "^0.7.5",
    "compodoc": "0.0.41",
    "core-js": "^3.6.4",
    "file-saver": "^2.0.2",
    "firebase": "^7.13.2",
    "jest-json-schema": "^2.1.0",
    "jquery": "^3.4.1",
    "jszip": "^3.3.0",
    "moment": "^2.24.0",
    "ng2-charts": "2.3.0",
    "ngx-clipboard": "^13.0.0",
    "ngx-markdown": "^9.0.0",
    "ngx-spinner": "^9.0.2",
    "ngx-toastr": "^12.0.1",
    "ngx-type-ahead": "^2.0.1",
    "popper.js": "^1.16.1",
    "rxjs": "~6.5.5",
    "rxjs-compat": "^6.5.5",
    "stream": "0.0.2",
    "tooltip.js": "^1.3.3",
    "tslib": "^1.10.0",
    "tslint-plugin-prettier": "^2.3.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/chart.js": "^2.9.18",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "protractor": "~5.4.3",
    "ts-node": "~8.8.2",
    "tslint": "~6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.8.3"
  },
  "compodoc": {
    "theme": "Readthedocs",
    "includes": "./src/assets/additional-docs/",
    "assetsFolder": "./src/assets/additional-docs/",
    "name": "Git4School documentation",
    "output": "tech-docs",
    "excludes": [
      "src/test.ts",
      "**/*.spec.ts"
    ]
  }
}
